{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "catkin_make",
            "args": [
                "--directory",
                "${workspaceFolder}",
                "-j4",
                "-l4",
                "-DCATKIN_WHITELIST_PACKAGES='robot_moveit_control'",
                "-DCMAKE_BUILD_TYPE=RelWithDebInfo",
                "-DPYTHON_EXECUTABLE=/usr/bin/python3"
            ],
            "problemMatcher": [
                "$catkin-gcc"
            ],
            "group": "build",
            "label": "catkin_make: build"
        },
        {
            "type": "cppbuild",
            "label": "C/C++: gcc 生成活动文件",
            "command": "/usr/bin/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "调试器生成的任务。"
        }
    ]
}